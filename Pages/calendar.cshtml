@page "{id:int?}"
@model calendarModel
@{
    var eventsJson = Newtonsoft.Json.JsonConvert.SerializeObject(Model.Events);
}
<!DOCTYPE html>
<html lang='en'>
<head>
    <meta charset='utf-8' />
    <script src='~/lib/fullcalendar/dist/index.global.min.js'></script>
    <script>

        document.addEventListener('DOMContentLoaded', function () {
            var calendarEl = document.getElementById('calendar');
            var calendar = new FullCalendar.Calendar(calendarEl, {
                initialView: 'dayGridMonth',
                events: @Html.Raw(eventsJson)
                          });
            calendar.render();
        });

    </script>
</head>
<body>
    <div id='calendar'></div>
</body>
</html>
